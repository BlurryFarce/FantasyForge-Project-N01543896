@model FantasyForge_N01543896.Models.UserMediaItemDto

@{
    ViewBag.Title = "User Media Item Details";
}

<h2>@ViewBag.Title</h2>

<div>
    <h4>User Media Item</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Title</dt>
        <dd>@Model.Title</dd>

        <dt>Status</dt>
        <dd>@Model.Status</dd>

        <dt>Rating</dt>
        <dd>@Model.Rating</dd>

        <dt>Review</dt>
        <dd>@Model.Review</dd>

        <dt>Type</dt>
        <dd>@Model.Type</dd>
